@model IEnumerable<WaterHistoryApp.Models.History>


<link rel="stylesheet" href="~/css/datepicker.css" />



    @using (Html.BeginForm("HistoryTable", "Table", FormMethod.Post))
    {
    <div class="container">

        <div class="date-box">
            <br />
            <label>Введите дату для просмотра таблицы со статистикой: </label><br />
            <input type='text' class='datepicker-here' name="Date" autocomplete="off" required />
            <button type="submit" class="btn btn-primary">Перейти</button>
        </div>
        
    </div>
    }

<script src="http://code.jquery.com/jquery-1.8.3.js"></script>
<script src="~/js/datepicker.js"></script>



